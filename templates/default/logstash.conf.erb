input {
  file {
    type => "robux.<%= @env %>"
    path => ["<%= @log_file %>"]
    debug => false
    codec => "<%= @log_codec %>"
    start_position => "end"
    stat_interval => 5
    tags => ["application", "<%= @env %>"]
  }
}
output {
  if [type] == "robux.<%= @env %>" {
    elasticsearch_http {
      host => "<%= @elastic_host %>"
      port => <%= @elastic_port %>
    }
  }
}
